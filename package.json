    "express": "4.16.4",
    "express-jwt": "^5.3.1",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "lerna": "^3.7.1",
    "mock-socket.io": "^1.1.0",
    "mongodb": "3.3.0-beta2",
