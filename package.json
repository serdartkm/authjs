    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "expect": "^24.9.0",
    "jest": "^24.9.0",
    "mock-socket": "^9.0.0",
    "sinon": "^7.4.2",
    "socket.io-client": "^2.2.0",
    "socket.io-mock": "^1.2.3"
  }
